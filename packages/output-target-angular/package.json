  "bugs": {
    "url": "https://github.com/baloise/stencil-ds-output-targets/issues"
  },
  "devDependencies": {
    "@angular/common": "^10.2.5",
    "@angular/core": "^10.2.5",
    "@angular/forms": "^10.2.5",
    "@angular/platform-browser": "^10.2.5",
    "@stencil/core": "^2.17.1",
    "jest": "^26.6.3",
    "rollup": "^2.56.3",
    "rxjs": "^6.6.7",
    "typescript": "^4.0.8",
    "zone.js": "^0.10.3"

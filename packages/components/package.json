{
  "name": "@baloise/design-system-components",
  "version": "3.0.0",
  "description": "The Baloise Design-System  is an open source project for styling awesome web applications that follow the Baloise corporate styling guidelines.",
  "repository": {
    "type": "git",
    "url": "https://github.com/baloise/design-system.git"
  },
  "homepage": "https://design.baloise.dev",
  "author": {
    "name": "Gery Hirschfeld",
    "email": "gerhard.hirschfeld@baloise.ch",
    "url": "https://github.com/hirsch88"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "design-system",
    "web-components",
    "stenciljs",
    "bulma-css"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/design-system-components/design-system-components.esm.js",
  "files": [
    "dist/",
    "src/styles/",
    "loader/"
  ],
  "scripts": {
    "start": "npm run serve",
    "update:registry": "sed -i -e \"s#nexus.balgroupit.com/repository/npm#registry.npmjs.org#g\" package-lock.json\n",
    "==================== BUILD ====================": "================================",
    "build": "npm run build:contributors && npm run build:testing && stencil build",
    "build:dev": "npm run build:contributors && stencil build --dev --config stencil.dev.config.ts",
    "build:contributors": "node .build/contributors.js",
    "build:testing": "node .build/testing.js",
    "================= DEVELOPMENT =================": "================================",
    "generate": "stencil generate",
    "dev": "npm run build:contributors && npm run dev:build && concurrently --names \"STENCIL,STORYBOOK\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm:dev:watch\" \"npm:dev:docs\"",
    "dev:build": "STORYBOOK_MODE=dev stencil build --config stencil.dev.config.ts",
    "dev:watch": "STORYBOOK_MODE=dev stencil build --config stencil.dev.config.ts --watch",
    "dev:debug": "STORYBOOK_MODE=dev stencil build --config stencil.dev.config.ts --dev",
    "dev:docs": "STORYBOOK_MODE=dev start-storybook -p 6006",
    "serve": "npm run build:contributors && npm run serve:build && concurrently --names \"STENCIL,STORYBOOK\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm:serve:watch\" \"npm:serve:docs\"",
    "serve:build": "STORYBOOK_MODE=prod stencil build --config stencil.dev.config.ts",
    "serve:watch": "STORYBOOK_MODE=prod stencil build --config stencil.dev.config.ts --watch",
    "serve:docs": "STORYBOOK_MODE=prod start-storybook -p 6006",
    "==================== TEST =====================": "================================",
    "test": "stencil test --spec --e2e --config stencil.test.config.ts",
    "test:watch": "stencil test --spec --e2e --watchAll --config stencil.test.config.ts",
    "test:unit": "stencil test --spec --config stencil.test.config.ts",
    "test:unit:watch": "stencil test --spec --watchAll --config stencil.test.config.ts",
    "test:e2e": "stencil test --e2e --config stencil.test.config.ts",
    "test:e2e:watch": "stencil test --e2e --watchAll --config stencil.test.config.ts",
    "log": "stencil build --prod --config stencil.log.config.ts --config stencil.test.config.ts",
    "www": "stencil build --config stencil.test.config.ts",
    "www:watch": "stencil build --dev --serve --config stencil.test.config.ts --watch",
    "================ DOCUMENTATION ================": "================================",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook -o ../../storybook"
  },
  "dependencies": {
    "@baloise/design-system-fonts": "^3.0.0",
    "@baloise/design-system-icons": "^3.0.0",
    "@baloise/web-app-utils": "^2.0.8",
    "@stencil/core": "^2.11.0",
    "bulma": "^0.9.3",
    "filesize.js": "^2.0.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.capitalize": "^4.2.1",
    "lodash.isarray": "^4.0.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isnil": "^4.0.0",
    "lodash.isstring": "^4.0.1",
    "lodash.lowercase": "^4.3.0",
    "lodash.round": "^4.0.4",
    "lodash.trim": "^4.5.1",
    "lodash.upperfirst": "^4.3.1"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@baloise/angular-output-target": "^1.0.19",
    "@baloise/vue-output-target": "^0.6.0",
    "@stencil/postcss": "^2.1.0",
    "@stencil/react-output-target": "^0.1.0",
    "@stencil/sass": "^1.5.2",
    "@storybook/addon-a11y": "^6.4.8",
    "@storybook/addon-essentials": "^6.4.8",
    "@storybook/addon-links": "^6.4.8",
    "@storybook/addons": "^6.4.8",
    "@storybook/theming": "^6.4.8",
    "@storybook/vue3": "^6.4.8",
    "@types/autoprefixer": "^10.2.0",
    "@types/jest": "^26.0.24",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.capitalize": "^4.2.6",
    "@types/lodash.isarray": "^4.0.6",
    "@types/lodash.isempty": "^4.4.6",
    "@types/lodash.isnil": "^4.0.6",
    "@types/lodash.isstring": "^4.0.6",
    "@types/lodash.lowercase": "^4.3.6",
    "@types/lodash.round": "^4.0.6",
    "@types/lodash.trim": "^4.5.6",
    "@types/lodash.upperfirst": "^4.3.6",
    "@types/puppeteer": "^5.4.4",
    "ag-grid-community": "^26.2.0",
    "babel-loader": "^8.2.3",
    "concurrently": "^6.4.0",
    "html-react-parser": "^1.4.2",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "js-beautify": "^1.14.0",
    "param-case": "^3.0.4",
    "prettier": "2.5.0",
    "puppeteer": "^10.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remark-codesandbox": "^0.10.0",
    "rimraf": "^3.0.2",
    "storybook-addon-designs": "^6.2.0",
    "storybook-addon-outline": "^1.4.2",
    "ts-dedent": "^2.2.0",
    "typescript": "^4.5.2",
    "vue": "^3.2.24",
    "vue-loader": "^16.8.3",
    "workbox-build": "^4.3.1"
  },
  "gitHead": "8bb3a41da0b577e5606f49a45e588a7bd4521eca"
}

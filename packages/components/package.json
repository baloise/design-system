{
  "name": "@baloise/design-system-next-components",
  "version": "10.21.2",
  "description": "The Baloise Design-System is an open source project for styling awesome web applications that follow the Baloise corporate styling guidelines.",
  "repository": {
    "type": "git",
    "url": "https://github.com/baloise/design-system.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/baloise/design-system/issues"
  },
  "homepage": "https://design.baloise.dev",
  "contributors": [
    "Gery Hirschfeld <gerhard.hirschfeld@baloise.ch> (https://github.com/hirsch88)",
    "Yannick Holzenkamp <yannick.holzenkamp@baloise.ch> (https://github.com/yannickholzenkamp)",
    "Petar Nobilo <petar.nobilo@baloise.ch> (https://github.com/nobilo)",
    "Laurent Tauber <laurent.tauber@baloise.ch> (https://github.com/Gagne87)",
    "Mladen Planinicic <mladen.planinicic@baloise.ch> (https://github.com/mladenplaninicic)",
    "Andreas Stebler <andreas.stebler@baloise.ch> (https://github.com/Clastzoo)"
  ],
  "keywords": [
    "design-system",
    "web-components",
    "stenciljs"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.esm.js",
  "es2017": "dist/esm/index.esm.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/design-system-components/design-system-components.esm.js",
  "files": [
    "dist/",
    "loader/",
    "src/styles/",
    "src/components/**/*.sass",
    "src/components/**/*.scss"
  ],
  "scripts": {
    "start": "npm run dev",
    "update:registry": "sed -i -e \"s#nexus.balgroupit.com/repository/npm#registry.npmjs.org#g\" package-lock.json\n",
    "==================== BUILD ====================": "================================",
    "build": "npm run build:meta && npm run build:stencil",
    "build:stencil": "stencil build --no-cache",
    "build:meta": "node ../../.build/docs-contributors.js && node ../../.build/docs-github.js && node ../../.build/docs-testing.js",
    "build:docs": "stencil build --config stencil.config.dev.ts && build-storybook -o ../../storybook  --quiet",
    "build:pre": "npm run build:meta && stencil build --config stencil.config.pre.ts",
    "================= DEVELOPMENT =================": "================================",
    "generate": "node ./.build/stencil/generate",
    "dev": "npm run dev:build && concurrently --names \"ðŸ§©_STENCIL__,ðŸ“•_STORYBOOK\" -c \"blue,magenta\" \"npm:dev:watch\" \"npm:dev:docs\"",
    "dev:build": "stencil build --config stencil.config.dev.ts",
    "dev:watch": "stencil build --config stencil.config.dev.ts --watch",
    "dev:docs": "start-storybook -p 6006 --quiet",
    "==================== TEST =====================": "================================",
    "test": "stencil test --spec",
    "test:watch": "stencil test --spec --watchAll",
    "serve": "stencil build --config stencil.config.e2e.ts --prod --serve --watch",
    "cy:build": "stencil build --config stencil.config.e2e.ts --prod",
    "cy:start": "node ./web-server.js",
    "cy:install": "cypress install",
    "cy:run": "cypress run --browser chrome --config excludeSpecPattern=\"cypress/e2e/visual/*cy.ts\"",
    "cy:open": "cypress open --browser chrome --config excludeSpecPattern=\"cypress/e2e/visual/*cy.ts\"",
    "cy:component:run": "cypress run --browser chrome --component --config excludeSpecPattern=\"cypress/e2e/visual/*cy.ts\"",
    "cy:visual:base": "cypress run --browser chrome --env type=base --spec \"cypress/e2e/visual/*cy.ts\" --config screenshotsFolder=cypress/snapshots/base",
    "cy:visual:actual": "cypress run --browser chrome --env type=actual --spec \"cypress/e2e/visual/*cy.ts\""
  },
  "dependencies": {
    "@baloise/design-system-next-css": "10.21.2",
    "@baloise/design-system-next-fonts": "10.21.2",
    "@baloise/design-system-next-icons": "10.21.2",
    "@baloise/design-system-next-tokens": "10.21.2",
    "@baloise/web-app-utils": "~3.6.1",
    "@popperjs/core": "~2.11.4",
    "@stencil/core": "~2.16.1",
    "big.js": "~6.2.1",
    "bulma": "~0.9.3",
    "date-fns": "~2.29.1",
    "filesize.js": "~2.0.0",
    "lodash.camelcase": "~4.3.0",
    "lodash.capitalize": "~4.2.1",
    "lodash.isarray": "~4.0.0",
    "lodash.isempty": "~4.4.0",
    "lodash.isnan": "~3.0.2",
    "lodash.isnil": "~4.0.0",
    "lodash.isstring": "~4.0.1",
    "lodash.padstart": "~4.6.1",
    "lodash.round": "~4.0.4",
    "lodash.upperfirst": "~4.3.1",
    "lottie-colorify": "0.8.0",
    "lottie-web": "5.8.1"
  },
  "devDependencies": {
    "@baloise/design-system-next-output-target-angular": "10.21.2",
    "@baloise/design-system-next-output-target-react": "10.21.2",
    "@baloise/design-system-next-output-target-vue": "10.21.2"
  },
  "gitHead": "34eccf2813e065bbb4d5a14cbc75581f5b485b40"
}

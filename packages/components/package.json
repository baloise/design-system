    "type": "git",
    "url": "https://github.com/baloise/design-system.git"
  },
  "homepage": "https://design.baloise.dev",
  "contributors": [
    "Gery Hirschfeld <gerhard.hirschfeld@baloise.ch> (https://github.com/hirsch88)",
    "Yannick Holzenkamp <yannick.holzenkamp@baloise.ch> (https://github.com/yannickholzenkamp)",
    "Petar Nobilo <petar.nobilo@baloise.ch> (https://github.com/nobilo)",
    "Laurent Tauber <laurent.tauber@baloise.ch> (https://github.com/Gagne87)"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "design-system",
    "web-components",
    "stenciljs",
    "bulma-css"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.esm.js",
  "es2017": "dist/esm/index.esm.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/design-system-components/design-system-components.esm.js",
  "files": [
    "dist/",
    "loader/",
    "src/styles/",
    "src/components/**/*.scss"
  ],
  "scripts": {
    "start": "npm run dev",
    "update:registry": "sed -i -e \"s#nexus.balgroupit.com/repository/npm#registry.npmjs.org#g\" package-lock.json\n",
    "==================== BUILD ====================": "================================",
    "build": "npm run build:vite && npm run build:meta && npm run build:testing && stencil build",
    "build:meta": "node .build/contributors.js && node .build/github.js",
    "build:testing": "node .build/testing.js",
    "build:docs": "stencil build --config stencil.dev.config.ts && build-storybook -o ../../storybook  --quiet",
    "build:vite": "baloiseStencilViteFix",
    "================= DEVELOPMENT =================": "================================",
    "generate": "node ./.build/stencil/generate",
    "dev": "npm run build:meta && npm run dev:build && concurrently --names \"ðŸ§©_STENCIL__,ðŸ“•_STORYBOOK\" -c \"blue,magenta\" \"npm:dev:watch\" \"npm:dev:docs\"",
    "dev:build": "stencil build --config stencil.dev.config.ts",
    "dev:watch": "stencil build --config stencil.dev.config.ts --watch",
    "dev:docs": "start-storybook -p 6006 --quiet",
    "==================== TEST =====================": "================================",
    "test": "stencil test --spec --e2e --config stencil.test.config.ts",
    "test:watch": "stencil test --spec --e2e --watchAll --config stencil.test.config.ts",
    "test:unit": "stencil test --spec --config stencil.test.config.ts",
    "test:unit:watch": "stencil test --spec --watchAll --config stencil.test.config.ts",
    "test:e2e": "stencil test --e2e --config stencil.test.config.ts",
    "test:e2e:watch": "stencil test --e2e --watchAll --config stencil.test.config.ts",
    "log": "stencil build --config stencil.log.config.ts",
    "www": "stencil build --config stencil.test.config.ts",
    "serve": "stencil build --config stencil.test.config.ts --dev --serve --watch",
    "cy:build": "npm run www",
    "cy:start": "node ./web-server.js",
    "cy:install": "cypress install",
    "cy:run": "cypress run --browser chrome --config ignoreTestFiles=\"visual-regression.spec.ts\"",
    "cy:open": "cypress open --browser chrome --config ignoreTestFiles=\"visual-regression.spec.ts\"",
    "cy:visual:base": "cypress run --browser chrome --env type=base --config screenshotsFolder=cypress/snapshots/base,testFiles=\"visual/*spec.ts\"",
    "cy:visual:actual": "cypress run --browser chrome --env type=actual --config testFiles=\"visual/*spec.ts\""
  },
  "dependencies": {
    "@baloise/design-system-fonts": "10.10.8",
    "@baloise/design-system-icons": "10.10.8",

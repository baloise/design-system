  "scripts": {
    "==================== START =========================": "================================",
    "serve": "stencil build --config stencil.config.e2e.ts --dev --serve --watch",
    "dev": "npm run docs:build && concurrently --names \"üß©_STENCIL__,üìï_STORYBOOK\" -c \"blue,magenta\" \"npm:dev:build\" \"npm:dev:server\"",
    "dev:build": "stencil build --config stencil.config.dev.ts --watch",
    "dev:server": "start-storybook -p 6006 --quiet",
    "==================== BUILD =========================": "================================",
    "build": "node ../../.build/docs-testing.js && node ../../.build/docs-contributors.js && stencil build --no-cache",
    "==================== UNIT TEST =====================": "================================",
    "test": "stencil test --spec",
    "test:watch": "stencil test --spec --watchAll",
    "==================== E2E TEST ======================": "================================",
    "e2e": "concurrently --names \"üß™_TEST__,üñ•Ô∏è__SERVER\" -c \"green,magenta\" \"npm:e2e:test\" \"npm:e2e:server\"",
    "e2e:test": "npm run cy:open",
    "e2e:server": "stencil build --config stencil.config.e2e.ts --serve --watch",
    "==================== CYPRESS =======================": "================================",
    "cy:install": "cypress install",
    "cy:open": "cypress open --browser chrome --env type=actual --config screenshotsFolder=cypress/snapshots/actual",
    "cy:build": "stencil build --config stencil.config.e2e.ts --prod",
    "cy:start": "node ./web-server.js",
    "==================== DOCUMENTATION =================": "================================",
    "docs": "npm run docs:build && build-storybook -o ../../storybook  --quiet",
    "docs:build": "node ../../.build/docs-github.js",
    "==================== RELEASE =======================": "================================",
    "prepublish": "node ../../.build/version.js"
  },
  "dependencies": {
    "@baloise/design-system-css": "12.1.0",

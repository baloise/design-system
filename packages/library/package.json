  "license": "Apache-2.0",
  "devDependencies": {
    "@stencil/postcss": "^1.0.1",
    "@stencil/sass": "^1.3.2",
    "@types/autoprefixer": "^9.7.2",
    "autoprefixer": "^10.0.1",
    "hirsch-angular-output-target": "0.0.6",
    "hirsch-vue-output-target": "0.0.5"
  },

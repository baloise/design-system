{
  "name": "design-system",
  "version": "0.0.0",
  "homepage": "https://design.baloise.dev",
  "license": "Apache-2.0",
  "packageManager": "bun@1.3.1",
  "scripts": {
    "start": "bunx nx run core:start",
    "docs": "bunx nx run docs:start",
    "e2e": "bunx nx run e2e:start",
    "play": "bunx nx run core:e2e-ui",
    "build": "bunx nx run-many -t build",
    "build:force": "bunx nx reset && bunx nx run-many -t build",
    "lint": "bunx nx run-many -t lint",
    "test": "bunx nx run-many -t test -- --watch=false",
    "format": "bunx nx format:write --all --end-of-line lf",
    "format:check": "bunx nx format:check --all --end-of-line lf",
    "spell": "cspell --no-progress",
    "registry": "bash registry",
    "changeset": "bunx nx run changeset",
    "optimize": "bunx nx run-many -t optimize",
    "angular": "bunx nx run angular-create",
    "angular:start": "bunx nx run angular-start",
    "react": "bunx nx run react-create",
    "react:start": "bunx nx run react-start"
  },
  "nx": {
    "includedScripts": []
  },
  "private": true,
  "workspaces": [
    "packages/*",
    "libs/*",
    "docs",
    "e2e"
  ],
